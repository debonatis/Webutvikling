<?xml version='1.0' encoding='UTF-8'?>

<!-- =========== FULL CONFIGURATION FILE ================================== -->

<faces-config version="2.1"
              xmlns="http://java.sun.com/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_1.xsd">
    <application>   
        <locale-config>
            <default-locale>en</default-locale>
            <supported-locale>no</supported-locale>
            <supported-locale>de</supported-locale>
        </locale-config>
        <resource-bundle>
            <base-name>Properties.texts</base-name>
            <var>texts</var>
        </resource-bundle>
    </application>
    <navigation-rule>
        <from-view-id>/user/input.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{treningsOktBehandling.oppdater()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/user/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <validator>
        <validator-id>sjekk</validator-id>
        <validator-class>com.corejsf.ValidatorTekst</validator-class>
    </validator>
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>#{treningsOktBehandling.oppdater()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/user/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>#{treningsOktBehandling.oppdater()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>#{bruker.logout()}</from-action>
            <from-outcome>ok</from-outcome>
            <to-view-id>/login/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>#{bruker.logout()}</from-action>
            <from-outcome>ok</from-outcome>
            <to-view-id>/login/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/useroverview.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bruker.oppdater()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/admin/useroverview.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>  
    <navigation-rule>
        <from-view-id>/login/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>ok</from-action>
            <from-outcome>ok</from-outcome>
            <to-view-id>/user/input.xhtml</to-view-id>
            <redirect />
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>adminok1</from-action>
            <from-outcome>adminok1</from-outcome>
            <to-view-id>/admin/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>adminok2</from-action>
            <from-outcome>adminok2</from-outcome>
            <to-view-id>/admin/changePassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>adminok3</from-action>
            <from-outcome>adminok3</from-outcome>
            <to-view-id>/admin/useroverview.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>userok1</from-action>
            <from-outcome>userok1</from-outcome>
            <to-view-id>/user/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>userok2</from-action>
            <from-outcome>userok2</from-outcome>
            <to-view-id>/user/changePassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>adminok3</from-action>
            <from-outcome>adminok3</from-outcome>
            <to-view-id>/admin/useroverview.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>adminok1</from-action>
            <from-outcome>adminok1</from-outcome>
            <to-view-id>/admin/overview1.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>adminok2</from-action>
            <from-outcome>adminok2</from-outcome>
            <to-view-id>/admin/changePassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/admin/*</from-view-id>
        <navigation-case>
            <from-action>#{bruker.changePassword()}</from-action>
            <from-outcome>ok</from-outcome>
            <to-view-id>/admin/changePassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/user/*</from-view-id>
        <navigation-case>
            <from-action>#{bruker.changePassword()}</from-action>
            <from-outcome>ok</from-outcome>
            <to-view-id>/user/changePassword.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule> 
    <navigation-rule>
        <from-view-id>/login/index.xhtml</from-view-id>
        <navigation-case>
            <from-action>#{bruker.oppdater()}</from-action>
            <from-outcome>success</from-outcome>
            <to-view-id>/login/index.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>      
</faces-config>
